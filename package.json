{
    "name": "Template parceljs_v1",
    "version": "1.0.0",
    "description": "a template for parcel js, sass, ",
    "targets": {
        "app": {
            "distDir": "./build"
        }
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "scripts": {
        "cleanup": "rm -f ./build/*",
        "watch": "npm run cleanup && parcel watch ./src/index.html --out-dir build ",
        "start": "npm run cleanup && parcel serve ./src/index.html --out-dir build ",
        "build": "npm run cleanup && parcel build ./src/index.html --out-dir build"
    },
    "author": "pil",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "7.13.8",
        "@babel/plugin-transform-runtime": "7.13.8",
        "@babel/preset-env": "7.13.8",
        "@babel/runtime": "7.13.8",
        "autoprefixer": "9.8.6",
        "parcel-bundler": "^1.3.1",
        "poetic": "^1.6.0",
        "postcss": "^8.2.8",
        "postcss-modules": "3.2.2",
        "sass": "^1.32.8"
    },
    "dependencies": {
        "@stomp/stompjs": "^6.1.0",
        "axios": "^0.21.1"
    },
    "browserslist": [
        "last 1 version",
        "last 5 Firefox versions",
        "last 5 Chrome versions",
        "last 5 iOS versions",
        "not dead",
        "not IE 11 and > 0.5%"
    ]
}
